	-$(DEL_FILE) "$(INSTALL_ROOT)$(HOME)/code/lib/i686/$(TARGET1)"
	-$(DEL_FILE) "$(INSTALL_ROOT)$(HOME)/code/lib/i686/$(TARGET2)"
	-$(DEL_DIR) "$(INSTALL_ROOT)$(HOME)/code/lib/i686/"


